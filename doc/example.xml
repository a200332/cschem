<qschem>
  <!-- This is a simple RC filter -->
  <circuit>
    <component x="10" y="15" type="resistor" rotation="0"
               name="R1" value="10k" id="001"/>
    <component x="25" y="24" type="capacitor:polarized" rotation="1"
               name="C1" value="100n" id="002"/>
    <component x="2" y="20" type="connector:bnc:in" rotation="1"
               name="J1" id="003" label="Input"/>
    <port x="25" y="27" name="Vee" type="ground" id="004"/>
    <component x="35" y="20" type="connector:bnc:out" rotation="1"
               name="J2" id="005" label="Output"/>
    <junction x="25" y="15" id="006"/>
    <junction x="25" y="25" id="007"/>
    <connection id="11" from="001:2" to="006"/>
    <connection id="12" from="006" to="002:pos"/>
    <connection id="13" from="006" to="005:signal"/>
    <connection id="14" from="002:neg" to="007"/>
    <connection id="15" from="003:signal" to="001:1"/>
    <connection id="16" from="003:ground" to="007" via="2,25"/>
    <connection id="17" from="005:ground" to="007" via="35,25"/>
    <connection id="20" from="007" to="004"/>
  </circuit>

  <parts>
    <package id="001" package="resistor:axial:0.4" vendor="DigiKey" partno="xxx-ND"/>
    <package id="002" package="capacitor:radial:0.1" vendor="Mouser" partno="..."/>
    <package id="003" package="connector:bnc:panel-ra-insulated"/>
    <package id="005" package="connector:bnc:panel-ra-insulated"/>
  </parts>
</qschem>
